{
	"apiVersion": 2,
	"name": "templaza-framework/advanced-products-filter",
	"version": "0.1.0",
	"title": "TemPlaza - Advanced Products Filter",
	"category": "widgets",
	"icon": "smiley",
	"description": "Generate to form from shortcode of advanced products plugin.",
	"supports": {
		"html": false
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Inventory Search"
		},
		"form_title": {
			"type": "string",
			"default": "Inventory Search"
		},
		"title_tag": {
			"type": "string",
			"default": "h3"
		},
		"title_display": {
			"type": "string",
			"default": "uk-display-block"
		},
		"title_padding": {
			"type": "array",
			"default": ""
		},
		"title_margin": {
			"type": "array",
			"default": ""
		},
		"enable_keyword": {
			"type": "boolean",
			"default": true
		},
		"enable_autocomplete": {
			"type": "boolean",
			"default": true
		},
		"limit_height": {
			"type": "boolean",
			"default": true
		},
		"enable_ajax": {
			"type": "boolean",
			"default": true
		},
		"update_url": {
			"type": "boolean",
			"default": true
		},
		"instant": {
			"type": "boolean",
			"default": false
		},
		"submit_text": {
			"type": "string",
			"default": "Search"
		},
		"submit_icon": {
			"type": "string",
			"default": ""
		},
		"ap_custom_fields": {
			"type": "array"
		},
		"column_large": {
			"type": "integer",
			"default": "1"
		},
		"column": {
			"type": "integer",
			"default": "1"
		},
		"column_laptop": {
			"type": "integer",
			"default": "1"
		},
		"column_tablet": {
			"type": "integer",
			"default": "1"
		},
		"column_mobile": {
			"type": "integer",
			"default": "1"
		},
		"max_height": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "templaza-framework",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
